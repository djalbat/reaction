{
  "name": "reaction",
  "author": "James Smith",
  "version": "4.0.19",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/reaction",
  "description": "An alternative implementation of React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/reaction"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.2.50",
    "esbuild": "^0.9.2",
    "express": "^4.17.1",
    "lively-cli": "^2.0.34",
    "watchful-cli": "^1.7.19"
  },
  "scripts": {
    "start": "node ./bin/main.js",
    "clean": "rm -rf ./lib",
    "watchful": "watchful -m --transpiler=swc --bundler=esbuild --source-directory=./src --lib-directory=./lib --entry-file=examples.js --bundle-file=./examples.js --wait=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
